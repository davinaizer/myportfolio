{
  "name": "myportfolio",
  "version": "1.1.0",
  "description": "Naizer portfolio",
  "main": "src/app/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --progress --hot --inline --open --mode development",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "clean": "rimraf dist && git worktree prune",
    "publish-gh": "cd dist && git add --all && git commit -m 'Deploy to gh-pages' && git push origin gh-pages",
    "deploy": "npm run clean && git worktree add dist gh-pages && npm run build && npm run publish-gh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davinaizer/myportfolio.git"
  },
  "keywords": [
    "Webpack",
    "4",
    "Reactjs"
  ],
  "author": "Davi Naizer",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/davinaizer/myportfolio/issues"
  },
  "homepage": "https://davinaizer.github.io/myportfolio/",
  "devDependencies": {
